<form>
  <p class="note">{{localize "IWS.SETTINGS.ITEM_EXCLUSION.HINT"}}</p>
  {{#each types}}
  <div class="form-group">
    <label for="item-link-tree-setting-{{value}}">{{label}}</label>
    <div class="form-fields">
      <input type="checkbox" name="{{value}}" id="item-link-tree-setting-{{value}}" {{checked checked}}>
    </div>
  </div>
  {{/each}}
  <button type="submit"><i class="fa-solid fa-save"></i> {{localize "Save"}}</button>
</form>
